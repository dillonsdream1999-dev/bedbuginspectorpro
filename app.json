{
  "expo": {
    "name": "Bed Bug Inspection Pro",
    "slug": "bed-bug-inspection-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1B2A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bedbug.inspectionpro",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos of potential bed bug hiding spots during your room inspection.",
        "NSPhotoLibraryUsageDescription": "This app saves inspection photos to your library for your records.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to connect you with local pest control professionals in your area.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D1B2A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bedbug.inspectionpro",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "This app uses the camera to take photos of potential bed bug hiding spots during your room inspection."
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "This app uses your location to connect you with local pest control professionals in your area."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "4c9b9548-e64d-4957-8da6-511b4a388afb"
      }
    },
    "owner": "dillonsdream199"
  }
}
